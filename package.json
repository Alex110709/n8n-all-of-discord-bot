{
	"name": "n8n-nodes-discord-all",
	"version": "2.1.1",
	"description": "Complete Discord Bot integration for n8n with all features - DM, Mentions, Server Management, Roles, Advanced Tools and more",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"discord",
		"discord-bot",
		"discord-api",
		"discord-dm",
		"discord-mentions",
		"discord-tools",
		"automation",
		"workflow"
	],
	"license": "MIT",
	"homepage": "https://github.com/alex110709/n8n-nodes-discord-all#readme",
	"author": {
		"name": "alex110709",
		"email": "alex110709@npmjs.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/alex110709/n8n-nodes-discord-all.git"
	},
	"bugs": {
		"url": "https://github.com/alex110709/n8n-nodes-discord-all/issues"
	},
	"main": "index.js",
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint src --ext .ts",
		"lintfix": "eslint src --ext .ts --fix",
		"prepublishOnly": "npm run build && npm run lint"
	},
	"files": [
		"dist",
		"docs",
		"LICENSE",
		"README.md"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/DiscordApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Discord/Discord.node.js",
			"dist/nodes/DiscordTrigger/DiscordTrigger.node.js",
			"dist/nodes/DiscordTools/DiscordTools.node.js",
			"dist/nodes/DiscordAdvanced/DiscordAdvanced.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^18.16.0",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"eslint": "^8.40.0",
		"eslint-plugin-n8n-nodes-base": "^1.12.0",
		"gulp": "^4.0.2",
		"n8n-workflow": "^1.0.0",
		"prettier": "^2.8.8",
		"typescript": "^5.0.4"
	},
	"dependencies": {
		"discord.js": "^14.14.1"
	}
}
